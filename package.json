{
  "name": "mama-mind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"blue,green\"",
    "build": "next build",
    "build:backend": "cd server && npm run build",
    "build:all": "npm run build && npm run build:backend",
    "start": "next start",
    "start:backend": "cd server && npm start",
    "lint": "next lint",
    "lint:backend": "cd server && npm run lint",
    "lint:all": "npm run lint && npm run lint:backend",
    "type-check": "tsc --noEmit",
    "type-check:backend": "cd server && npm run type-check",
    "type-check:all": "npm run type-check && npm run type-check:backend",
    "test": "jest",
    "test:backend": "cd server && npm test",
    "test:all": "npm run test && npm run test:backend",
    "test:watch": "jest --watch",
    "setup": "node database/setup.js",
    "setup:dev": "node setup-dev.js",
    "env:setup": "node scripts/setup-env.js",
    "env:test": "node scripts/validate-env.js",
    "dev:check": "npm run env:test && npm run dev:all",
    "db:test": "node database/test-connection.js",
    "db:migrate": "echo 'Run the SQL files in your Supabase dashboard'",
    "db:seed": "echo 'Run database/sample-data.sql in your Supabase SQL editor'",
    "check-deps": "npm ls && cd server && npm ls",
    "clean": "rm -rf .next dist node_modules/.cache server/dist server/node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules server/node_modules"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@supabase/supabase-js": "^2.50.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.1",
    "framer-motion": "^12.19.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "recharts": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
